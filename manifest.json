{
  "manifest_version": 2,
  "name": "AWSDocReader",
  "description": "Read management of AWS documents",
  "version": "1.0.0",
  "homepage_url": "https://github.com/enomotodev/AWSDocReader",

  "permissions": [
    "storage"
  ],

  "content_scripts": [{
      "matches": ["https://docs.aws.amazon.com/*"],
      "js": ["jquery.js", "script.js"]
  }]
}
